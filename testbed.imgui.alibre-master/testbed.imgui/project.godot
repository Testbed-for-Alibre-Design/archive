; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters
config_version=5
[application]
config/name="testbed.imgui"
run/main_scene="res://data/Init.tscn"
config/features=PackedStringArray("4.1", "C#")
boot_splash/image="res://src/logo.png"
boot_splash/fullsize=false
config/icon="res://data/icon.svg"
[autoload]
ImGuiLayer="*res://addons/imgui-godot/ImGuiLayer.tscn"
[display]
window/size/viewport_width=1280
window/size/viewport_height=720
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
[dotnet]
project/assembly_name="testbed.imgui"
[editor_plugins]
enabled=PackedStringArray("res://addons/imgui-godot/plugin.cfg")
[rendering]
environment/defaults/default_clear_color=Color(0, 0, 0, 1)
